<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/extenstion_navigation"
    app:startDestination="@id/extenstionFragment">

    <fragment
        android:id="@+id/extenstionFragment"
        android:name="com.example.codebaseandroidapp.ui.ExtensionFragment"
        android:label="ExtenstionFragment" >
        <action
            android:id="@+id/action_extenstionFragment_to_serviceFragment"
            app:destination="@id/serviceFragment" />
        <action
            android:id="@+id/action_extenstionFragment_to_workManagerFragment"
            app:destination="@id/workManagerFragment" />
        <action
            android:id="@+id/action_extenstionFragment_to_demoSolidFragment"
            app:destination="@id/demoSolidFragment" />
        <action
            android:id="@+id/action_extenstionFragment_to_demoThreadProcessFragment"
            app:destination="@id/demoThreadProcessFragment" />
        <action
            android:id="@+id/action_extenstionFragment_to_demoReactiveExtensionFragment"
            app:destination="@id/demoReactiveExtensionFragment" />
    </fragment>
    <fragment
        android:id="@+id/serviceFragment"
        android:name="com.example.codebaseandroidapp.ui.ServiceFragment"
        android:label="ServiceFragment" />
    <fragment
        android:id="@+id/workManagerFragment"
        android:name="com.example.codebaseandroidapp.ui.WorkManagerFragment"
        android:label="fragment_work_manager"
        tools:layout="@layout/fragment_work_manager" />
    <fragment
        android:id="@+id/demoSolidFragment"
        android:name="com.example.codebaseandroidapp.ui.DemoSolidFragment"
        android:label="DemoSolidFragment" />
    <fragment
        android:id="@+id/demoThreadProcessFragment"
        android:name="com.example.codebaseandroidapp.ui.DemoThreadProcessFragment"
        android:label="DemoThreadProcessFragment" />
    <fragment
        android:id="@+id/demoReactiveExtensionFragment"
        android:name="com.example.codebaseandroidapp.ui.DemoReactiveExtensionFragment"
        android:label="DemoReactiveExtensionFragment" />
</navigation>